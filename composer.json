{
    "name": "rollerworks/datagrid-twig",
    "description": "Twig render engine for RollerworksDatagrid",
    "keywords": ["rollerworks", "datagrid", "twig"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/datagrid-twig/contributors"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "rollerworks/datagrid": "^0.8.1",
        "twig/twig": "~1.23",
        "symfony/twig-bridge": "~2.3|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Datagrid\\Twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Datagrid\\Tests\\Twig\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
