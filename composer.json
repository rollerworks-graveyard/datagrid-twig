{
    "name": "rollerworks/datagrid-twig",
    "description": "Twig render engine for RollerworksDatagrid",
    "keywords": ["rollerworks", "datagrid", "twig"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/datagrid-twig/contributors"
        }
    ],
    "require": {
        "php": "^7.0",
        "rollerworks/datagrid": "^0.11",
        "twig/twig": "^1.26.1",
        "symfony/twig-bridge": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5.4",
        "phpspec/prophecy": "^1.6.1",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Datagrid\\Twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Datagrid\\Twig\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev"
        }
    }
}
